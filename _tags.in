<*/*.ml> or <*/*.mli>: pp(camlp4o Camlp4MacroParser.cmo)
<*/*.ml> or <*/*.mli>: package(extlib), @PKG_CUDF@, package(pcre)

<doseparse/boilerplate.ml>: pp(camlp4o Camlp4MacroParser.cmo @CONFIG_ZIP@ @CONFIG_BZ2@ @CONFIG_RPM@ @CONFIG_DB@ @CONFIG_OCAMLGRAPH@)
<doseparse/boilerplateNorpm.ml>: pp(camlp4o Camlp4MacroParser.cmo @CONFIG_ZIP@ @CONFIG_BZ2@ @CONFIG_DB@ @CONFIG_OCAMLGRAPH@)

<debian/*.{ml,mli}>: use_common
<rpm/*.ml> or <rpm/*.mli>: use_common
<algo/*.ml> or <algo/*.mli>: package(ocamlgraph), use_common
<eclipse/*.ml> or <eclipse/*.mli>: use_common, use_debian
<doseparse/boilerplate.ml>: use_common, @USE_RPM@, use_debian, use_eclipse
<doseparse/boilerplateNoRpm.ml>: use_common, use_debian, use_eclipse

<common/*.cmx>: for-pack(Common)
<algo/*.cmx>: for-pack(Algo)
<debian/*.cmx>: for-pack(Debian)
<eclipse/*.cmx>: for-pack(Eclipse)
<rpm/*.cmx>: for-pack(Rpm)

<applications/*>: package(extlib), @PKG_CUDF@, package(pcre), package(str), package(unix), @PKG_OCAMLGRAPH@, @PKG_ZIP@, @PKG_BZ2@

<applications/apt-backend.*>: use_common, use_debian, use_eclipse, use_boilerplateNoRpm
<applications/distcheck.*>: use_common, use_debian, @USE_RPM@, use_eclipse, use_algo, use_boilerplate
<applications/deb-buildcheck.*>: use_common, use_debian, use_eclipse, use_algo, use_boilerplateNoRpm
<applications/ceve.*>: use_common, use_debian, use_eclipse, @USE_RPM@, use_algo, use_boilerplate
<applications/strong-conflicts.*>: use_common, use_debian, use_eclipse, @USE_RPM@, use_algo, use_boilerplate
<applications/strong-deps.*>: use_common, use_debian, use_eclipse, use_algo, @USE_RPM@, use_boilerplate
<applications/smallworld.*>: use_common, use_debian, use_eclipse, @USE_RPM@, use_algo, use_boilerplate
